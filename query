1) SELECT * 
FROM `students`
WHERE `date_of_birth` LIKE '1990%';

2) SELECT * 
FROM `courses`
WHERE `cfu` > 10;

3) SELECT *
FROM `students`
WHERE `date_of_birth` < '1993-03-28';

4) SELECT * 
FROM `courses`
WHERE `period` LIKE 'I %' AND `year` = 1;

5) SELECT * 
FROM `exams`
WHERE `hour` >= '14:00:00' AND `date` = '2020-06-20';

6) SELECT * FROM `degrees`
WHERE `level` = 'magistrale';

7) SELECT COUNT(*) AS total_departments
FROM `departments`;