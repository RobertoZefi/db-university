1) SELECT COUNT(id) AS Iscrizioni, YEAR(`enrolment_date`)
FROM `students`
GROUP BY YEAR(`enrolment_date`);

2) SELECT COUNT(id) AS Insegnanti, office_address
FROM `teachers`
GROUP BY office_address;

3) SELECT AVG(vote), exam_id
FROM `exam_student`
GROUP BY exam_id;

4) SELECT COUNT(name), department_id
FROM `degrees`
GROUP BY department_id;